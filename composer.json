{
    "name": "mewesk/twig-excel-bundle",
    "type": "symfony-bundle",
    "description": "This Symfony bundle provides a PhpExcel integration for Twig",
    "keywords": ["excel","phpexcel","twig","symfony"],
    "homepage": "https://github.com/MewesK/TwigExcelBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Mewes Kochheim",
            "email": "mewes@kochheim.de"
        },
        {
            "name": "Community Contributors",
            "homepage": "https://github.com/MewesK/TwigExcelBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": "5.5.9 - 7.2",
        "symfony/framework-bundle": "~2.8|~4.0",
        "phpoffice/phpexcel": "^1.8"
    },
    "require-dev": {
        "symfony/symfony": "~2.8|~4.0",
        "symfony/phpunit-bridge": "^4.0",
        "sensio/framework-extra-bundle": "~5.1",
        "mpdf/mpdf": "^7.0"
    },
    "suggest": {
        "mpdf/mpdf": "Add support for PDF rendering, requires ~7.0"
    },
    "autoload": {
        "psr-4": { "MewesK\\TwigExcelBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
